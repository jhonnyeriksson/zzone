<config-template xmlns="http://tail-f.com/ns/config/1.0"
                 servicepoint="lab">
  <devices xmlns="http://tail-f.com/ns/ncs">
    <?foreach {/devices}?>
      <device>
        <name>{.}</name>
        <config>
          <hostname xmlns="urn:ios">{hostnames/hostname_new}</hostname>
        </config>
      </device>
    <?end?>
  </devices>
</config-template>