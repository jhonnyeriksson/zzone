<config-template xmlns="http://tail-f.com/ns/config/1.0"
                 servicepoint="lab">
  <devices xmlns="http://tail-f.com/ns/ncs">
    <?foreach {/target-device}?>
      <device>
        <name>{.}</name>
        <config>
          <?foreach{/hostnames}?>
            <?if {. = node}?>
              <hostname xmlns="urn:ios">YAY</hostname>
            <?end?>
          <?end?>
        </config>
      </device>
    <?end?>
  </devices>
</config-template>